package terrace.vo;
/**
 * 대학교 정보에 대한 Value Object
 */
public class UnivVO {
	//member variable
	private String univName;//대학명
	private String univLoc;//지역
	private String univEmail;//대학명에 따른 @이하 이메일 주소
	
	//constructor
	public UnivVO() {
		super();
	}

	public UnivVO(String univName, String univLoc, String univEmail) {
		super();
		this.univName = univName;
		this.univLoc = univLoc;
		this.univEmail = univEmail;
	}

	
	//setter/getter
	public String getUnivName() {
		return univName;
	}

	public void setUnivName(String univName) {
		this.univName = univName;
	}

	public String getUnivLoc() {
		return univLoc;
	}

	public void setUnivLoc(String univLoc) {
		this.univLoc = univLoc;
	}

	public String getUnivEmail() {
		return univEmail;
	}

	public void setUnivEmail(String univEmail) {
		this.univEmail = univEmail;
	}

	//toString override
	@Override
	public String toString() {
		return "UnivVO [univName=" + univName + ", univLoc=" + univLoc
				+ ", univEmail=" + univEmail + "]";
	}

}
